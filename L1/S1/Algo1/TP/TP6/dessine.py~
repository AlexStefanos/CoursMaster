import turtle

# Code de configuration de la fenetre
picture_width  = 2000
picture_height = 500
turtle.setup (width=picture_width, height=picture_height)
# Fin de code de configuration de la fenetre

# Ici commence le code pour dessiner
turtle.penup()
turtle.goto(-900,100)
turtle.pendown()
turtle.left(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(100)
turtle.backward(100)
turtle.left(90)
turtle.forward(50)
turtle.penup()
turtle.left(90)
turtle.forward(75)
turtle.pendown()
turtle.left(90)
turtle.forward(100)
turtle.penup()
turtle.backward(100)
turtle.pendown()
turtle.right(90)
turtle.forward(100)
turtle.penup()
turtle.forward(75)
turtle.left(90)
turtle.pendown()
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.penup()
turtle.backward(100)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.pendown()
turtle.forward(100)
turtle.backward(100)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(100)
# Code d'attente pour fermer la fenetre
turtle.hideturtle ()
turtle.exitonclick ()
